{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "api/user/{**catch-all}"
        }
      },
      "route2": {
        "ClusterId": "cluster2",
        "Match": {
          "Path": "api/home/{**catch-all}"
        }
      },
      "route3": {
        "ClusterId": "cluster3",
        "Match": {
          "Path": "api/email/{**catch-all}"
        }
      },
      "route4": {
        "ClusterId": "cluster4",
        "Match": {
          "Path": "hubs/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:4001/"
          }
        }
      },
      "cluster2": {
        "Destinations": {
          "destination2": {
            "Address": "https://localhost:4000/"
          }
        }
      },
      "cluster3": {
        "Destinations": {
          "destination3": {
            "Address": "https://localhost:4002/"
          }
        }
      }
    }
  }
}
