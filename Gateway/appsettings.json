{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "api/user/{**catch-all}"
        }
      },
      "route2": {
        "ClusterId": "cluster2",
        "Match": {
          "Path": "api/home/{**catch-all}"
        }
      },
      "route3": {
        "ClusterId": "cluster3",
        "Match": {
          "Path": "api/email/{**catch-all}"
        }
      },
      "route4": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "chat-hub/{**catch-all}"
        }
      },
      "route5": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "api/Chats/{**catch-all}"
        }
      },
      "route6": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "api/payments/{**catch-all}"
        }
      },
      "route7": {
        "ClusterId": "cluster4",
        "Match": {
          "Path": "api/images/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:3001/"
          }
        }
      },
      "cluster2": {
        "Destinations": {
          "destination2": {
            "Address": "http://localhost:3000/"
          }
        }
      },
      "cluster3": {
        "Destinations": {
          "destination2": {
            "Address": "http://localhost:3002/"
          }
        }
      },
      "cluster4": {
        "Destinations": {
          "destination4": {
            "Address": "http://localhost:3003/"
          }
        }
      }
    }
  }
}